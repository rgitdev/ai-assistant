{
  "name": "bun-react-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.tsx",
  "module": "src/index.tsx",
  "scripts": {
    "dev": "bun --hot src/index.tsx",
    "build": "bun build ./src/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
    "start": "NODE_ENV=production bun src/index.tsx",
    "webapp": "bun --hot webapp/index.tsx",
    "frontend": "bun --hot frontend/src/index.tsx",
    "webapp2": "bun --hot webapp2/src/server.tsx",
    "backend": "bun --hot backend/index.tsx"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.15.1",
    "@types/uuid": "^10.0.0",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.5",
    "framer-motion": "^12.23.12",
    "langfuse": "^3.38.5",
    "lucide-react": "^0.542.0",
    "openai": "^5.17.0",
    "qdrant": "^1.3.1",
    "react": "^19",
    "react-dom": "^19",
    "uuid": "^12.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/bun": "latest"
  }
}
